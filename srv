fd.pipe	local-log.r	local-log.w
service.args	local-log	tinylog	-t	-k	1	log/local
service.fds	local-log	local-log.r	stderr	stderr
service.auto_up	local-log	2	always

service.args	local	sudo	-u	frew	/home/frew/code/daemonproxy/build/daemonproxy	-c	frew-srv	-i
service.fds	local	null	local-log.w	local-log.w
service.auto_up	local	1	always

service.args	my_exit_script	perl	/home/frew/code/daemonproxy/doc/example-clean-shutdown-controller.pl
service.fds	my_exit_script	control.event	control.cmd	stderr
service.auto_up	my_exit_script	2	SIGTERM	SIGINT	SIGQUIT

